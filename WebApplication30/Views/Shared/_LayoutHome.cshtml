                            <span>
                                <a href="~/GioHang/Index"><i class="fa fa-shopping-cart" aria-hidden="true"></i></a>

                            </span>
                            @if (Session["TaiKhoan"] != null)
                            {
                                int sl = 0;
                                var tk = Session["TaiKhoan"] as WebApplication30.Models.KhachHang;
                                var db = new WebApplication30.DAO.HoaDonDAO();
                                var hd = db.FindHD(tk.MaKH);
                                if (hd != null)
                                {

                                    var list = db.GetListCTHD(hd.MaHD);
                                    if (list != null)
                                    {
                                        foreach (var item in list)
                                        {
                                            sl = sl + item.SoLuong;
                                        }
                                    }
                                }
                                <span class="quntity">@sl</span>
                            }
                            else
                            {
                                <span class="quntity">0</span>
                            }
                            
                        </div>

                        <form method="post" action="~/Home/TimKiem" class="form-inline my-2 my-lg-0">
                            <input class="form-control mr-sm-2" type="search" name="tenMH" placeholder="Tìm kiếm." aria-label="Search">
                            <span>
                                <button type="submit" style="border-radius:4px;font-size:12px;">Tìm</button>
                                @*<a onclick="this.form.submit()"><i class="fa fa-shopping-cart" aria-hidden="true"></i></a>*@
                            </span>
                            @*<a onclick="this.form.submit()"><span class="fa fa-search"></span></a>*@
