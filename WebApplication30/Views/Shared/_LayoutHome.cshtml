                            @if (Session["TaiKhoan"] == null)
                            {
                                <li class="navbar-item">
                                    <a href="~/DangNhap/Index" class="nav-link">Đăng Nhập</a>
                                </li>
                            }
                            else
                            {
                                var tk = Session["TaiKhoan"] as WebApplication30.Models.KhachHang;

                                <li class="navbar-item">
                                    <a href="~/Account/Index" class="nav-link">Tài Khoản</a>
                                </li>
                                if (tk.LoaiTK == "Admin")
